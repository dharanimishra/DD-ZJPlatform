/*table formatting*/
.alt-table {
            width: 100%; 
			margin:10px 0 5px 0;
			border:1px solid #ddd;
        }
		.alt-table td{ padding:9px; vertical-align:top; text-align:left; font-size:120%;}
		.alt-table th{ vertical-align:top;  font-weight:bold; 	padding:10px;  font-size:120%;}
        .alt-table thead {
            background-color: #69C;
            color: white;
            text-align: left;
		
        }
        .alt-table tbody tr:nth-child(odd) {
            background-color: #fff; /* red, 30% alpha */
			color:#666;
			
        }
        .alt-table tbody tr:nth-child(even) {
            background-color: #efefef; /* green, 10% alpha */
			color:#555;
        }