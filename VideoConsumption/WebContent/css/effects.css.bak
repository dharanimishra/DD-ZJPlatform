@charset "utf-8";


.all-box-shadow{
	-moz-box-shadow: 3px 4px 4px -3px #D9D9D9;
	-webkit-box-shadow: 3px 4px 4px -3px #D9D9D9;
	box-shadow: 3px 4px 4px -3px #D9D9D9;
	}

.bottom-box-shadow{
	-moz-box-shadow: 0px 1px 1px #d9d9d9;
	-webkit-box-shadow: 0px 1px 1px #d9d9d9;
	box-shadow: 0px 1px 1px #d9d9d9;
	}
.top-box-shadow{
	-moz-box-shadow: 0px -1px 1px #d9d9d9;
	-webkit-box-shadow: 0px -1px 1px #d9d9d9;
	box-shadow: 0px -1px 1px #d9d9d9;
	}
.for-rounded-box{
border-top-left-radius: 2px;
border-top-right-radius: 2px;
border-bottom-right-radius: 2px;
border-bottom-left-radius: 2px;

-moz-border-radius: 2px;
-moz-border-radius-topleft: 2px;
-moz-border-radius-topright: 2px;
-moz-border-radius-bottomright: 2px;
-moz-border-radius-bottomleft: 2px;
}
.two-top-rounded-box{
border-top-left-radius: 2px;
border-top-right-radius: 2px;

-moz-border-radius-topleft: 2px;
-moz-border-radius-topright: 2px;
}
.two-bottom-rounded-box{
border-bottom-right-radius: 2px;
border-bottom-left-radius: 2px;

-moz-border-radius-bottomright: 2px;
-moz-border-radius-bottomleft: 2px;
}
.two-left-rounded-box{
border-top-left-radius: 4px;
border-bottom-left-radius: 4px;

-moz-border-radius-topleft: 4px;
-moz-border-radius-bottomleft: 4px;
}
.two-right-rounded-box{
border-bottom-right-radius: 4px;
border-top-right-radius: 4px;

-moz-border-radius-bottomright: 4px;
-moz-border-radius-topright: 4px;
}


.border-right{
	border-right-width: 1px;
	border-right-style: solid;
	border-right-color: #cccccc;	
}
.border-left{
	border-left-width: 1px;
	border-left-style: solid;
	border-left-color: #cccccc;
}
.border-top{
	border-top-width: 1px;
	border-top-style: solid;
	border-top-color: #cccccc;
}
.border-bottom{
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-bottom-color: #cccccc;
}



 
