<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>
	<typeAliases>
		<typeAlias type="com.springs.domain.FileVO" alias="file"/>
		<typeAlias type="com.springs.domain.FileInfoVO" alias="fileInfo"/>
		<typeAlias type="com.springs.domain.PlayList" alias="playlist"/>
		<typeAlias type="com.springs.domain.BookMark" alias="bookmark"/>
		<typeAlias type="com.springs.domain.Enrichment" alias="enrichment"/>
		<typeAlias type="com.springs.domain.ComponentNest" alias="componentNest"/>
		<typeAlias type="com.springs.domain.ComponentType" alias="componentType"/>
	</typeAliases>
</configuration>
